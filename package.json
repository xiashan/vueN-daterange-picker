{
  "name": "vuen-daterange-picker",
  "version": "1.0.0-rc.7",
  "description": "just a date range picker based on element ui",
  "main": "dist/vuen-daterange-picker.js",
  "scripts": {
    "docs:dev": "yarn concurrently \"yarn components:parse\" \"yarn components:watch\" \"vuepress dev docs\"",
    "docs:build": "yarn components:parse && vuepress build docs",
    "components:parse": "babel-node ./docs/.scripts/parse.js",
    "components:watch": "watch \"yarn components:parse\" ./src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .js,.vue --ignore-path .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .",
    "build": "NODE_ENV=production rollup -c",
    "pub": "sh ./publish.sh"
  },
  "author": "xiashan",
  "license": "ISC",
  "dependencies": {
    "element-ui": "^2.12.0",
    "vuen-popper": "^1.0.0-rc.3"
  },
  "devDependencies": {
    "@babel/node": "^7.7.4",
    "@vuedoc/parser": "^2.0.0-beta.5",
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.0",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "prettier": "^1.19.1",
    "raw-loader": "^3.1.0",
    "rollup": "^1.27.5",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "^5.1.4",
    "vue": "^2.6.10",
    "vue-codemirror": "^4.0.6",
    "vuepress": "^1.2.0",
    "watch": "^1.0.2"
  }
}
